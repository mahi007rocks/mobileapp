<link href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<script src="//netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js"></script>
<script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
<script src="//rawgithub.com/stidges/jquery-searchable/master/dist/jquery.searchable-1.0.0.min.js"></script>
<!------ Include the above in your HEAD tag ---------->

<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4 phone">
      <div class="row1">
        <div class="col-md-12 back-grnd">
          <div class="contact-list">
            <ul class="list-group" id="contact-list">
              <% @contacts.each do |user| %>
                <li class="list-group-item">
                  <div class="col-xs-12 col-sm-3">
                    <img src="http://www.webcoderskull.com/img/team4.png" alt="Scott Stevens" class="img-responsive img-circle" />
                  </div>
                  <div class="col-xs-12 col-sm-9">
                    <span class="name"><%= user.name%> </span><br/>
                    <span class="text-muted"><%= user.phonenumber%></span> 
                  </div>
                  <div class="clearfix"></div>
                </li>
              <%end %>
            </ul>
          </div>
          <input type="tel" name="name" id="telNumber" class="form-control tel" value="" />
            <div class="num-pad">
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          1
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          2 <span class="small">
                              <p>
                                  ABC</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          3 <span class="small">
                              <p>
                                  DEF</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          4 <span class="small">
                              <p>
                                  GHI</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          5 <span class="small">
                              <p>
                                  JKL</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          6 <span class="small">
                              <p>
                                  MNO</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          7 <span class="small">
                              <p>
                                  PQRS</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          8 <span class="small">
                              <p>
                                  TUV</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          9 <span class="small">
                              <p>
                                  WXYZ</p>
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          *
                      </div>
                  </div>
              </div>
              <div class="span4">
                  <div class="num">
                      <div class="txt">
                          0 <span class="small">
                          </span>
                      </div>
                  </div>
              </div>
              <div class="span4">
                <div class="num">
                  <div class="txt">   #
                </div>
              </div>
            </div>
          </div>
          <div class="clearfix"></div>
          <br>
        </div>
      </div>
      <div class="clearfix"></div>
    </div>
  </div>
</div>
